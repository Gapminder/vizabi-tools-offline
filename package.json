{
  "name": "GapminderOffline",
  "version": "0.13.5",
  "description": "Gapminder Offline",
  "repository": {
    "type": "git",
    "url": "http://gapminder.org/tools"
  },
  "author": "Vyacheslav Chub",
  "main": "electron.js",
  "scripts": {
    "build-chrome": "NODE_ENV=development webpack",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:osx && npm run build:win && npm run build:linux",
    "build:osx": "scripts/build-macos.js \"Vizabi Offline\" \"Gapminder\" 0.1.0 2016",
    "build:win": "scripts/build-win.js \"Vizabi Offline\" \"Gapminder\" 0.1.0 2016",
    "build:linux": "scripts/build-linux.js \"Vizabi Offline\" \"Gapminder\" 0.1.0 2016"
  },
  "build": {
    "app-bundle-id": "org.develar.onshape",
    "app-category-type": "public.app-category.graphics-design",
    "osx": {
      "title": "Vizabi Offline",
      "icon": "./build/icon.icns"
    },
    "win": {
      "title": "Vizabi Offline",
      "icon": "./build/icon.ico"
    },
    "linux": {
      "title": "Vizabi Offline",
      "icon": "./build/icon.ico"
    }
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.3",
    "compression-webpack-plugin": "0.2.0",
    "css-loader": "0.20.2",
    "electron-builder": "3.9.0",
    "electron-packager": "7.0.1",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.4",
    "html-loader": "0.3.0",
    "html-webpack-plugin": "1.6.1",
    "image-webpack-loader": "1.6.2",
    "imagemin": "3.2.1",
    "imports-loader": "valorkin/imports-loader",
    "shelljs": "0.6.0",
    "style-loader": "0.13.0",
    "stylus": "0.54.2",
    "stylus-loader": "2.0.0",
    "url-loader": "0.5.6",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.1"
  },
  "dependencies": {
    "URLON": "1.0.1",
    "angular": "1.4.7",
    "angular-route": "1.4.7",
    "angular-touch": "1.4.7",
    "async": "1.5.0",
    "body-parser": "1.14.1",
    "bourbon": "4.2.6",
    "compression": "1.6.0",
    "connect-gzip-static": "1.0.0",
    "d3": "3.5.6",
    "electron-prebuilt": "0.37.5",
    "express": "4.13.3",
    "express-redis-cache": "0.1.8",
    "format-json": "1.0.3",
    "lodash": "3.10.1",
    "method-override": "2.3.5",
    "mongoose": "4.1.12",
    "morgan": "1.6.1",
    "node-bourbon": "4.2.3",
    "redis": "2.2.5",
    "vizabi": "0.13.8-25",
    "web-fs": "2.1.0"
  }
}
